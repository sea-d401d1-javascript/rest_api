
{
  "rules": {
    "indent": [2, 2],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "no-console": 0,
    "no-path-concat": 0,
    "operator-assignment": 0,
    "no-inline-comments": 0,
    "max-len": 0,
    "curly": 0,
    "quote-props": 0,
    "no-unused-vars": 0
  },
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": "eslint:recommended"
}
